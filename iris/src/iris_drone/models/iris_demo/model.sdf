            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="iris_laserscan" filename="libgazebo_ros_ray_sensor.so">
          <ros>
            <remapping>~/out:=scan</remapping>
          </ros>
          <output_type>sensor_msgs/LaserScan</output_type>
          <frame_name>base_link</frame_name>
        </plugin>
      </sensor>
